<%= render 'books/book',user: @user, book: @book %>

<h1>User</h1>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
    <td><%= image_tag user.get_profile_image,size: "100x100" %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show",user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
  <tfoot>
  </tfoot>
</table>